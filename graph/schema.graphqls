# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Pirate {
  id: ID!
  name: String! 
  bounty: String! 
  crew: String!
}

type Query {
  pirates: [Pirate!]! 
}

input NewPirate {
  name: String! 
  pirateId: String! 
  bounty: String!
  crew: String! 
}

type Mutation {
  createPirate(input: NewPirate!): Pirate!
}
